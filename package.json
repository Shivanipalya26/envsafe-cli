{
  "name": "envsafe-cli",
  "version": "1.0.1",
  "description": "A CLI tool to list and validate environment variable keys from .env files.",
  "main": "bin/index.js",
  "bin": {
    "envsafe-cli": "bin/index.js"
  },
  "files": [
    "dist", 
    "bin"
  ],
