  "packageManager": "pnpm@10.6.3",
  "devDependencies": {
    "@types/node": "^24.2.0",
    "tsx": "^4.20.3",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "boxen": "^8.0.1",
    "chalk": "^5.5.0",
    "figures": "^6.1.0"
  }
